<Window x:Class="AutoFiller.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Autofiller" Height="400" Width="400">
    <Grid>
        <StackPanel>
            <Button Command="{Binding UpdateCommand}" Content="Update Data" Margin="10"/>
            <Button Command="{Binding FillStatisticsCommand}" Content="Fill Statistics" Margin="10"/>
            <Button Command="{Binding FillSatDataCommand}" Content="Fill Sat Data" Margin="10"/>
            
            <ListBox x:Name="LogsListBox" ItemsSource="{Binding Logs}" Margin="10" Height="200"/>
        </StackPanel>
    </Grid>
</Window>